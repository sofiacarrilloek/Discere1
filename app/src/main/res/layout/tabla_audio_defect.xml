<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tabla.Taudio_defect">

    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center">
        <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Speech Analyst Detail"
            android:layout_marginTop="15dp"
            android:textSize="24dp"
            ></TextView>


        <HorizontalScrollView android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            >

            <LinearLayout android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:orientation="vertical"
                        >
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:textSize="18dp"
                            android:text="#"
                            />

                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="50dp"
                        android:orientation="vertical"
                        >
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:textSize="18dp"
                            android:text="Duration"
                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="50dp"
                        android:orientation="vertical"
                        >
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:textSize="18dp"
                            android:text="Defect Priority"
                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="50dp"
                        android:orientation="vertical"
                        >
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:textSize="18dp"
                            android:text="Defect Type"
                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="50dp"
                        android:orientation="vertical"
                        >
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:textSize="18dp"
                            android:text="Defect Description"
                            />
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout android:layout_width="50dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"
                        android:background="@color/colorLOL"

                        >
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1"
                            android:textSize="20dp"
                            android:layout_gravity="center"
                            />

                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"
                        android:background="@color/colorLOL"
                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:id="@+id/Duration1"
                            android:layout_gravity="center"

                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"
                        android:background="@color/colorLOL"
                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:id="@+id/DP1"

                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"
                        android:background="@color/colorLOL"
                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:id="@+id/DT1"

                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"
                        android:background="@color/colorLOL"
                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:id="@+id/DD1"

                            />
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout android:layout_width="50dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"

                        >
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="2"
                            android:textSize="20dp"
                            android:layout_gravity="center"
                            />

                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"

                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:id="@+id/Duration2"
                            android:layout_gravity="center"

                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"

                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:id="@+id/DP2"

                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"

                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:id="@+id/DT2"

                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"

                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:id="@+id/DD2"

                            />
                    </LinearLayout>


                </LinearLayout>
                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout android:layout_width="50dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"
                        android:background="@color/colorLOL"

                        >
                        <TextView android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="3"
                            android:textSize="20dp"
                            android:layout_gravity="center"
                            />

                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"
                        android:background="@color/colorLOL"
                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:id="@+id/Duration3"
                            android:layout_gravity="center"

                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"
                        android:background="@color/colorLOL"
                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:id="@+id/DP3"

                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"
                        android:background="@color/colorLOL"
                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:id="@+id/DT3"

                            />
                    </LinearLayout>
                    <LinearLayout android:layout_width="200dp"
                        android:layout_height="90dp"
                        android:orientation="vertical"
                        android:background="@color/colorLOL"
                        >
                        <EditText android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:id="@+id/DD3"

                            />
                    </LinearLayout>

                </LinearLayout>





            </LinearLayout>





        </HorizontalScrollView>
        <Button
            android:id="@+id/pasaelzelda2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Regresar"
            android:onClick="regresarX"

            />




    </LinearLayout>


</LinearLayout>